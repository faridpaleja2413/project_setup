{
  "name": "final_touch_project",
  "version": "1.0.0",
  "description": "",
  "main": "build/index.js",
  "scripts": {
    "build": "rimraf ./build && tsc && npm run copy-build",
    "build-dev": "rimraf ./build && npm run copy-build && npm run watch",
    "copy-build": "copyfiles .env* locales/i18n/*.json src/email-templates/templateFiles/*/* src/email-templates/templateFiles/*/*/* package.json* build",
    "start": "set NODE_ENV=local && ts-node-dev src/index.ts",
    "start:consumer": "NODE_ENV=local nodemon build/stream/consumers/index.js",
    "start:consumer:dev": "NODE_ENV=local ts-node -r tsconfig-paths/register -P ./tsconfig.dev.json -T stream/consumers/notifications.ts",
    "start:dev": "NODE_ENV=local nodemon",
    "start:docker": "docker composer up",
    "dev": "nodemon src/index.ts",
    "watch": "tsc -w",
    "test": "jest --detectOpenHandles --coverage --verbose",
    "test:coverage": "jest --coverage",
    "test:coverage:ci": "npm run test:coverage --silent --noStackTrace --ci --bail --json",
    "format": "npm run lint:fix && npm run prettier:fix",
    "lint": "eslint . --ext .ts",
    "lint:changes": "func() { eslint $(git diff --name-only --diff-filter=ACMRTUXB origin/$1 | grep  -E \"(.js$|.ts$|.tsx$)\"); }; func",
    "lint:fix": "eslint . --ext .ts --fix",
    "prettier:check": "prettier --config .prettierrc src/**/*.ts --check",
    "prettier:fix": "prettier --config .prettierrc **/**/*.ts --write",
    "migration:run": "ts-node --project ./tsconfig.json -r tsconfig-paths/register node_modules/typeorm/cli.js migration:run",
    "migration:revert": "ts-node --project ./tsconfig.json -r tsconfig-paths/register node_modules/typeorm/cli.js migration:revert",
    "migration:create": "ts-node --project ./tsconfig.json -r tsconfig-paths/register node_modules/typeorm/cli.js migration:create --name"
  },
  "_moduleAliases": {
    "@middleware": "build/src/middleware",
    "@db": "build/src/utils/db",
    "@utils": "build/src/utils",
    "@config": "build/src/config.js",
    "@constants": "build/src/constants",
    "@server": "build/src/server.js",
    "@testUtils": "build/testUtils",
    "@entities": "build/schemas/entities",
    "@common": "build/src/api/common",
    "@models": "build/models/index",
    "@community": "build/src/api/community",
    "@queue": "build/queue",
    "@stream": "build/stream",
    "@userSessionWatchTime": "build/src/api/userSessionWatchTime"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.20.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "cls-hooked": "^4.2.2",
    "cors": "^2.8.5",
    "dotenv": "^16.0.1",
    "express": "^4.17.1",
    "express-basic-auth": "^1.2.0",
    "express-pino-logger": "6.0.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "mysql": "^2.18.1",
    "mysql2": "^2.3.3",
    "newrelic": "^8.14.1",
    "nodemon": "^2.0.18",
    "pino": "^8.1.0",
    "pino-pretty": "4.8.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "routing-controllers": "^0.9.0",
    "typedi": "^0.10.0",
    "typeorm": "^0.2.35",
    "typeorm-naming-strategies": "^4.1.0",
    "typeorm-typedi-extensions": "^0.4.1",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/cls-hooked": "^4.3.3",
    "@types/dotenv": "^8.2.0",
    "@types/express": "4.17.7",
    "@types/express-pino-logger": "4.0.2",
    "@types/node": "^18.0.0",
    "ts-node": "8.3.0",
    "ts-node-dev": "1.1.6",
    "typescript": "^4.7.4"
  }
}
